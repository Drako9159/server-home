
<div id="items" class="items">
    <h3>Files</h3>
    <% filesPrivate.forEach((e) =>{ %>
    <div class="card">
        <div class="card-left">
            <p><b>Titulo:</b> <%- e.title %></p>
            <p><b>Creado:</b> <%- e.createdAt %></p>
            <p><b>Tipo:</b> <%- e.tipo %></p>
            <p><b>Tama√±o:</b> <%- e.size %></p>
        </div>
        <div class="card-center"></div>
        <div class="card-right">
            <a href="/files/edit/<%- e.id %>">
                <img src="./icons/editar.png" />
            </a>
            <a href="/files/share/<%- e.id %>" class='<%- e.share ? "background-share-active" : "background-share-desactive" %>'>
                <img src="./icons/share.png" />
            </a>
            <a href="/files/delete/<%- e.id %>">
                <img src="./icons/basura.png" />
            </a>
        </div>
    </div>
    <% }) %>
</div>