<%- include("./partials/head"); %>
  <body>
    <header><%- include("./partials/navigation") %></header>

    <section class="form-container">
      <form action="new-file" enctype="multipart/form-data" method="POST">
        <div class="secuencial-container">
          <label for="title">Nombre del archivo</label>
          <input
            autofocus
            type="text"
            required
            name="title"
            id="title"
            placeholder="Nombre de Archivo"
          />
        </div>

        <div class="secuencial-container">
          <label for="files" class="charge-image">Archivo</label>
          <input type="file" class="image" name="files" id="files" />
        </div>

        <input type="submit" value="Guardar" class="btn-submit" />
       
      </form>
    </section>
    
    <%- include("./partials/footer"); %>
  </body>

  
</html>
